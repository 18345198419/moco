[
  {
    "request" : {
      "uri" : "/record"
    },
    "response" : {
      "record" : "name"
    }
  },
  {
    "request" : {
      "uri" : "/replay"
    },
    "response" : {
      "replay" : "name"
    }
  },
  {
    "request" : {
      "uri" : "/record-template"
    },
    "response" : {
      "record" : {
        "template": "${req.queries['type']}"
      }
    }
  },
  {
    "request" : {
      "uri" : "/replay-template"
    },
    "response" : {
      "replay" : {
        "template": "${req.queries['type']}"
      }
    }
  }
]